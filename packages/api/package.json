{
  "name": "@covidtop/api",
  "version": "0.0.0",
  "repository": "https://github.com/covidtop/covidtop/tree/master/packages/api",
  "license": "MIT",
  "main": "lib/main.js",
  "scripts": {
    "clean": "dev clean",
    "eslint": "dev eslint src",
    "prettier": "dev prettier 'src/**/*.ts'",
    "build": "dev build",
    "start-production": "NODE_CONFIG_DIR=../../config NODE_ENV=production node lib/main.js",
    "start-dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "../../tmp/build-info.txt"
    ],
    "env": {
      "NODE_CONFIG_DIR": "../../config",
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "@covidtop/calculator": "0.0.0",
    "@covidtop/data": "0.0.0",
    "@covidtop/shared": "0.0.0",
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/schedule": "0.3.1",
    "config": "3.3.1",
    "dashify": "2.0.0",
    "download": "8.0.0",
    "helmet": "3.22.0",
    "node-object-hash": "2.0.0",
    "reflect-metadata": "0.1.13",
    "string-similarity": "4.0.1"
  },
  "devDependencies": {
    "@covidtop/dev": "0.0.0",
    "@types/config": "0.0.36",
    "@types/dashify": "1.0.0",
    "@types/download": "6.2.4",
    "@types/helmet": "0.0.45",
    "@types/string-similarity": "3.0.0",
    "nodemon": "2.0.3"
  }
}
